package {	import flash.display.*;	import flash.events.*;	import Content_Manager3;		public class Add_Page4 extends MovieClip {		private var my_cm:Content_Manager3;				public function Add_Page4() {			var close_tween:Object	= { x:560, time:1, transition:"easeInCubic" };					// Move the object to y 500 to get it off the stage			var open_tween:Object	= { x:26, time:1, transition:"easeInCubic", delay:1 };	// Move the object on to the stage to y 124			var start_values:Object	= { x:-400, y:26	};									my_cm = new Content_Manager3( this, open_tween, close_tween, start_values );						b_0.addEventListener( MouseEvent.CLICK, b_0_click );			b_1.addEventListener( MouseEvent.CLICK, b_1_click );		}						private function b_0_click( evt:Event ):void {			my_cm.add_content( Gallery );		}				private function b_1_click( evt:Event ):void {			my_cm.add_content( Gallery2 );		}			}}